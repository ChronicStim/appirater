source 'https://github.com/ChronicStim/ChronicStim-Specs.git'
source 'https://github.com/CocoaPods/Specs.git'

# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Appirater' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Appirater
  pod 'SCLAlertView-CPT', :git => 'https://github.com/ChronicStim/SCLAlertView.git', :branch => 'CPT_version_4_0_x', :inhibit_warnings => true

end
